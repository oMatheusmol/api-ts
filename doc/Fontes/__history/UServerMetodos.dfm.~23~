        'INNER JOIN Municipios ON UnidadesCidadeAtendimento.CodigoMunicip' +
        'io = Municipios.CodigoMunicipio'
      
        'INNER JOIN EmpresaUnidades ON UnidadesCidadeAtendimento.CodigoUn' +
